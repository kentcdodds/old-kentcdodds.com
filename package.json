{
  "name": "kentcdodds.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "my personal website",
  "main": "index.js",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:copy": "npm-run-all --parallel build:copy:*",
    "build:copy:copyDir":
      "cpy \"**/*.*\" \"**/.well-known/*.*\" ../dist/ --parents --cwd=./copy",
    "build:copy:posts":
      "cpy \"**/*.!(js)\" ../../../../dist/post --parents --cwd=./src/pages/blog/posts",
    "build:pages": "babel-node generate/",
    "build": "npm-run-all --parallel build:*",
    "test": "npm run lint",
    "lint": "kcd-scripts lint",
    "serve": "http-server dist",
    "validate": "kcd-scripts validate",
    "precommit": "kcd-scripts precommit",
    "storybook": "cross-env STORYBOOK_RUNNING=true start-storybook --port 9001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentcdodds/kentcdodds.github.io.git"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/kentcdodds.github.io/issues"
  },
  "homepage": "https://github.com/kentcdodds/kentcdodds.github.io#readme",
  "devDependencies": {
    "@reactivex/rxjs": "^6.0.0",
    "@storybook/react": "^3.4.2",
    "babel-cli": "^6.24.0",
    "babel-macros": "^1.2.0",
    "babel-plugin-react-require": "^3.0.0",
    "common-tags": "^1.7.2",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.1.4",
    "cross-spawn": "^6.0.5",
    "eslint-config-kentcdodds": "^13.0.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.12.4",
    "glob": "^7.1.1",
    "http-server": "^0.11.1",
    "import-all.macro": "^2.0.3",
    "kcd-scripts": "^0.38.0",
    "marked": "^0.3.19",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "random-emoji": "^1.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "rxjs": "^5.2.0",
    "slugify": "^1.2.9",
    "strip-indent": "^2.0.0",
    "uglify-js": "^3.3.22"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/kcd-scripts/eslint.js",
      "kentcdodds/babel-react-require"
    ],
    "rules": {
      "react/jsx-max-props-per-line": "off"
    }
  },
  "eslintIgnore": ["node_modules", "dist"]
}
